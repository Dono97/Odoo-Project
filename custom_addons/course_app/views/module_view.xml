<?xml version="1.0"?>
   <odoo>
     <record id="view_form_module" model="ir.ui.view">
       <field name="name">Module View</field>
       <field name="model">module.program</field>
       <field name="arch" type="xml">
         <form string="Module">
           <header>
             <button name="compute_module_code" type="object"
               string="Create module code" />
           </header>
           <sheet>
             <group>
               <field name="name" />
               <field name="year" />
               <field name="code" />
               <!-- <field name="code" /> -->
             </group>
           </sheet>
         </form>
       </field>
     </record>
</odoo>
